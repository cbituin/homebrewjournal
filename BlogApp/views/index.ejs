<% include partials/header %>

<h1>Index page!</h1>

<% blogs.forEach(function(blog){ %>
<div>
    <h2><%=blog.title%></h2>
    <img src="<%=blog.image%>">
    <span><%=blog.created%></span>
    <p><%=blog.styleName%></p>
    <a href="/blogs/<%= blog._id %>">Read More</a>

</div>
<% }) %>

<% include partials/footer %>